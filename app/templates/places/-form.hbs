<form {{action "save" on="submit"}}>
  <div>
    <label>
      Name
      {{input type="text" value=model.name}}
    </label>
  </div>
  <div>
    <label>
      Country
      {{input type="text" value=model.country}}
    </label>
  </div>
  <div>
    <input type="submit" value="Save">
  </div>
</form>
